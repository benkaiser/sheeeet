{
  "name": "sheeeet",
  "version": "0.0.1",
  "homepage": "https://github.com/benkaiser/sheeeet",
  "authors": [
    "Benjamin Kaiser <benjaminjkaiser@gmail.com>"
  ],
  "description": "Timesheet management application",
  "main": "app.js",
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "backbone": "~1.1.2",
    "bootbox": "~4.3.0",
    "bootstrap": "~3.2.0",
    "bootswatch": "v3.2.0+4",
    "headjs": "~1.0.3",
    "jquery": "~2.1.1",
    "marionette": "~2.2.2",
    "messenger": "~1.4.1",
    "moment": "~2.8.3",
    "civswig": "civitaslearning/swig-bower#~1.3.2",
    "underscore": "~1.7.0"
  },
  "resolutions": {
    "underscore": "~1.7.0"
  },
  "install": {
    "path": {
      "css": "static/css/libs",
      "js": "static/js/libs"
    },
    "sources": {
      "backbone": "bower_components/backbone/backbone.js",
      "bootbox": "bower_components/bootbox/bootbox.js",
      "bootstrap": "bower_components/bootstrap/dist/js/bootstrap.min.js",
      "bootswatch": "bower_components/bootswatch/cosmo/bootstrap.min.css",
      "civswig": "bower_components/civswig/swig.min.js",
      "headjs": "bower_components/headjs/dist/1.0.0/head.load.min.js",
      "jquery": "bower_components/jquery/dist/jquery.min.js",
      "marionette": "bower_components/marionette/lib/backbone.marionette.min.js",
      "messenger": [
        "bower_components/messenger/build/css/messenger-theme-air.css",
        "bower_components/messenger/build/js/messenger.min.js"
      ],
      "moment": "bower_components/moment/min/moment.min.js",
      "underscore": "bower_components/underscore/underscore-min.js"
    }
  }
}
